@{
  Layout="Shared/_Layout.cshtml";
}

<h1>To Do List</h1>
<hr />
<h2>Object1s:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var object1 in Model)
    {
      <li><a href="/object1s/@object1.GetId()">@object1.GetName()</a></li>
    }
  </ul>
}
<h4><a href="/object1s/new">Add a new object1</a></h4>
<h4><a href="/">Return home</a></h4>
