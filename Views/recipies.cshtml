@{
  Layout="Shared/_Layout.cshtml";
}

<h2>Recipies</h2>
<hr />
@if (Model.Count != 0)
{
  <ul>
    @foreach (var recipie in Model)
    {
      <li><a href='/recipies/@recipie.GetId()'>@recipie.GetName()</a>---@recipie.GetRating() stars</li>
    }
  </ul>
}
<h4><a href="/recipies/new">Add new recipie</a></h4>
<h4><a href="/">Return home</a></h4>
