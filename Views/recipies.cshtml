@{
  Layout="Shared/_Layout.cshtml";
}

<h1>Recipie Box</h1>
<hr />
<h2>Recipies:</h2>
@if (Model.Count != 0)
{
  <ul>
    @foreach (var recipie in Model)
    {
      <li><a href='/recipies/@recipie.GetId()'>@recipie.GetName()</a></li>
    }
  </ul>
}
<h4><a href="/recipies/new">Add new recipie</a></h4>
<h4><a href="/">Return home</a></h4>
